<configuration>
    <configSections>
        <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Unity.Configuration"/>
    </configSections>
    <unity>
        <sectionExtension type="Microsoft.Practices.Unity.InterceptionExtension.Configuration.InterceptionConfigurationExtension, Unity.Interception.Configuration"/>
        <containers>
            <container name="cyfContainer">
                <extension type="Interception"/>
                <register type="System.Data.Entity.DbContext, EntityFramework" mapTo="Cyf.EntityFramework.Model.CyfDBContext, Cyf.EntityFramework.Model"/>
                <register type="Cyf.EntityFramework.Interface.ICompanyService,Cyf.EntityFramework.Interface" mapTo="Cyf.EntityFramework.Business.CompanyService, Cyf.EntityFramework.Business">
                    <interceptor type="InterfaceInterceptor"/>
                </register>
                <register type="Cyf.EntityFramework.Interface.IUserService,Cyf.EntityFramework.Interface" mapTo="Cyf.EntityFramework.Business.UserService, Cyf.EntityFramework.Business">
                    <interceptor type="InterfaceInterceptor"/>
                    <interceptionBehavior type="Cyf.MVC5.Utility.AOP.LogBeforeBehavior, Cyf.MVC5"/>
                    <interceptionBehavior type="Cyf.MVC5.Utility.AOP.LogAfterBehavior, Cyf.MVC5"/>
                </register>
            </container>

            <!--<container name="ruanmouContainerGeneric">
                <register type="System.Data.Entity.DbContext, EntityFramework" mapTo="Ruanmou.EF.Model.JDDbContext, Ruanmou.EF.Model"/>
                <register type="Ruanmou.Bussiness.Interface.IBaseService`1,Ruanmou.Bussiness.Interface" mapTo="Ruanmou.Bussiness.Service.BaseService`1, Ruanmou.Bussiness.Service"/>
                <register type="Ruanmou.Bussiness.Interface.ICategoryService,Ruanmou.Bussiness.Interface" mapTo="Ruanmou.Bussiness.Service.CategoryService, Ruanmou.Bussiness.Service"/>
                <register type="Ruanmou.Bussiness.Interface.ICommodityService,Ruanmou.Bussiness.Interface" mapTo="Ruanmou.Bussiness.Service.CommodityService, Ruanmou.Bussiness.Service"/>
            </container>-->
        </containers>
    </unity>
</configuration>